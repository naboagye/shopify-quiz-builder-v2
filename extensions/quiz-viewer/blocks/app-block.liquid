{% render "quiz-page", quiz_id: block.settings.quiz_id, quiz_bg_img: block.settings.quiz_bg_img  %}
{% schema %}
  {
    "name": "Quiz",
    "target": "section",
    "stylesheet": "quiz.css",
    "javascript": "product-quiz.js",
    "settings": [
        {
        "type": "text",
        "id": "quiz_id",
        "label": "Quiz ID"
       },
       {
        "type": "image_picker",
        "id": "quiz_bg_img",
        "label": "Quiz Background Image"
      }
    ]
  }
{% endschema %}
